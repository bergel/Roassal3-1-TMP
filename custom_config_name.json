{
  "scale": 2,
  "vertical_spacing": 1,
  "width": 140,
  "microprint_width": 140,
  "number_of_columns": 4,

  "default_colors": {
    "background_color": "rgb(30, 30, 30)",
    "text_color": "white"
  },
  "line_rules": {
    "error": {
      "text_color": "red",
      "background_color": "white"
    },
    "[command]": {
      "text_color": "white",
      "background_color": "green"
    },
    "command": {
      "text_color": "purple"
    },
    "NewUndeclaredWarning": {
      "text_color": "black",
      "background_color": "yellow"
    },
    "MetacelloNotification": {
      "text_color": "black",
      "background_color": "orange"
    },
    "complete": {
      "text_color": "green",
      "background_color": "white"
    }
  }
}
